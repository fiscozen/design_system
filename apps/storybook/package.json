    "@fiscozen/badge": "workspace:^",
    "@fiscozen/breadcrumbs": "workspace:^",
    "@fiscozen/button": "workspace:^",
    "@fiscozen/card": "workspace:^",
    "@fiscozen/checkbox": "workspace:^",
    "@fiscozen/collapse": "workspace:^",
    "@fiscozen/composables": "workspace:^",
    "@fiscozen/container": "workspace:^",
    "@fiscozen/datepicker": "workspace:^",
    "@fiscozen/dialog": "workspace:^",
    "@fiscozen/divider": "workspace:^",
    "@fiscozen/dropdown": "workspace:^",
    "@fiscozen/icons": "workspace:^",
    "@fiscozen/input": "workspace:^",
    "@fiscozen/layout": "workspace:^",
    "@fiscozen/link": "workspace:^",
    "@fiscozen/navbar": "workspace:^",
    "@fiscozen/navlink": "workspace:^",
    "@fiscozen/navlist": "workspace:^",
    "@fiscozen/pdf-viewer": "workspace:^",
    "@fiscozen/progress": "workspace:^",
    "@fiscozen/radio": "workspace:^",
    "@fiscozen/select": "workspace:^",
    "@fiscozen/simple-table": "workspace:^",
    "@fiscozen/stepper": "workspace:^",
    "@fiscozen/style": "workspace:^",
    "@fiscozen/tab": "workspace:^",
    "@fiscozen/table": "workspace:^",
    "@fiscozen/textarea": "workspace:^",
    "@fiscozen/toast": "workspace:^",
    "@fiscozen/tooltip": "workspace:^",
    "@fiscozen/topbar": "workspace:^",
    "@fiscozen/typeahead": "workspace:^",
    "@fiscozen/upload": "workspace:^",
    "@fiscozen/view-flag": "workspace:^",
    "vue": "^3.4.13",
    "vue-router": "^4.3.0"
