    "autoprefixer": "^10.4.16",
    "chromatic": "^13.3.0",
    "cross-var": "^1.1.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^9.1.10",
    "jsdom": "^23.0.1",
